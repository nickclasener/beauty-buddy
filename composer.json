{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/PurposeBuilt/laravel-scout-elastic"
    }
  ],
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "ajthinking/tinx": "^2.1",
    "cviebrock/eloquent-sluggable": "^4.5",
    "elasticsearch/elasticsearch": "^6.7",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.7.*",
    "laravel/tinker": "^1.0",
    "nothingworks/blade-svg": "^0.2.2",
    "tightenco/ziggy": "^0.6.9"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "barryvdh/laravel-ide-helper": "^2.4",
    "beyondcode/laravel-dump-server": "^1.2",
    "brianium/paratest": "^2.1",
    "codedungeon/phpunit-result-printer": "^0.26.0",
    "doctrine/dbal": "^2.9",
    "filp/whoops": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.14",
    "fzaninotto/faker": "^1.4",
    "laravel/dusk": "^4.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "nunomaduro/larastan": "^0.3.15",
    "nunomaduro/phpinsights": "^1.3",
    "phpunit/phpunit": "^7.5",
    "spatie/phpunit-watcher": "^1.8",
    "sven/artisan-view": "^3.1"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/utilities/functions.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": [
      "tests/utilities/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan clear-compiled",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:models -N",
      "php artisan ide-helper:meta",
      "php artisan optimize:clear"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
